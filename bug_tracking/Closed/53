FILE: ./simple.csp:
/*OUTPUT FROM the aStraDG2 COMPILER, an *.sdg to *.csp */
/*compiler written by Terry Tateyama and Eric Peskin.  */
/*Questions, comments, requests, & bug reports welcome!*/
/************************ mailto:peskin@cs.utah.edu ****/
module simple;
    output	GETXreq	= {false};
     input	GETXack	= {false};


    process explicitSimple;
        *[
            [~GETXack -> GETXreq+];
            [ GETXack -> GETXreq-]
         ]
    endprocess

    process env;
        *[
            [ GETXreq -> GETXack+];
            [~GETXreq -> GETXack-]
         ]
    endprocess

endmodule
              
              
