FILE: ./hb.csp:
/*****************************************************************************/
/* Martin's half-buffer circuit (see ARVLSI97).                              */
/*****************************************************************************/
module hb;

delay gatedelay = <90,110>;

input l0={gatedelay};
input l1={gatedelay};
input re={gatedelay};
output le={true,gatedelay};
output r0={gatedelay};
output r1={gatedelay};

process main;
*[ [re]; [ l0 -> r0+ | l1 -> r1+ ]; le-; [~re]; [~l0 & ~l1]; 
	[r0 -> r0- | r1 -> r1-]; le+ ]
endprocess

process r;
*[re+; [r0 | r1]; re-; [~r0 & ~r1]]
endprocess

process l;
*[[ skip -> l0+; [~le]; l0-; [le]
  | skip -> l1+; [~le]; l1-; [le]
 ]]
endprocess
endmodule
              
              
